 <script>
// Function to get region_id from URL
function showLoader() {
  
  document.getElementById('loader').style.display = 'flex'; // Show loader

}

// Function to hide the loader
function hideLoader() {
  document.getElementById('loader').style.display = 'none'; // Hide loader
}

function getRegionIdFromUrl() {
    const urlParams = new URLSearchParams(window.location.search);
    const region_id=urlParams.get('region_id')
    return region_id; // Adjust key if needed
}

// Function to fetch member data
async function fetchRegionData(regionId) {
    showLoader();
    try {
        const response = await fetch(`https://bni-data-backend.onrender.com/api/getRegion/${regionId}`);
        if (!response.ok) {
            toastr.error("Something went wrong try again")
            throw new Error(`Error: ${response.statusText}`);
        }
        const regionData = await response.json();
        console.log(regionData)
        displayRegionData(regionData); // Function to handle the data
    } catch (error) {
        console.error("Failed to fetch member data:", error);
    }finally{
        hideLoader()
    }
}

// Function to display member data on the page
async function displayRegionData(data, regionId) {
    const selectedRegion = data
console.log(selectedRegion)
    if (selectedRegion) {
        // Update profile image or use a default if none available
        document.getElementById("profileImage").src = "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTXnDsD4WCge-cUF9IJysBS57H8nHw2y1gNDw&s";

        // Update card with member details
        document.getElementById("regionName").textContent = `${selectedRegion.region_name}`;
        document.getElementById("regionType").textContent = selectedRegion.chapter_type || "Not Provided";

        // Update location details
        document.getElementById("regionCity").textContent = selectedRegion.state || "Not Available";
        document.getElementById("regionCountry").textContent = selectedRegion.country || "India";

        // Basic Info section
        document.getElementById("regionLaunchedBy").textContent = `${selectedRegion.region_launched_by}`;
        document.getElementById("regionStatus").textContent = `${selectedRegion.region_status}` || "Not Provided";
        document.getElementById("contactEmail").textContent = selectedRegion.email_id || "Not Provided";
        document.getElementById("contactPhone").textContent = selectedRegion.contact_number || "Not Provided";
        document.getElementById("contactPerson").textContent = selectedRegion.contact_person || "Not Provided";
// profile info
document.getElementById("bio1").textContent = selectedRegion.vision||""`
`

document.getElementById("bio2").textContent = selectedRegion.mission || `The ${selectedRegion.region_name} region of BNI is dedicated to fostering strong business relationships and sharing opportunities for mutual growth. Known for its collaborative approach, commitment to excellence, and passion for helping businesses succeed, this region provides a vibrant platform for professionals to connect, learn, and thrive. Through BNI, members of the ${selectedRegion.region_name} region look forward to expanding their networks, exchanging insights, and contributing to the collective success of the community.`;

document.getElementById("companyEmail").textContent = selectedRegion.email_id || "Not Provided";
document.getElementById("companyWebsite").textContent = selectedRegion.member_website || "Not Provided";
document.getElementById("companyLocation").textContent = `${selectedRegion.street_address_line_1}` || "Not Provided";
document.getElementById("companyPhone").textContent = `${selectedRegion.member_phone_number},${selectedRegion.member_alternate_mobile_number}` || "Not Provided";
document.getElementById("youtubeLink").textContent =selectedRegion.social_youtube|| "Not Available";
document.getElementById("twitterLink").textContent =selectedRegion.social_twitter|| "Not Available";
document.getElementById("linkedinLink").textContent =selectedRegion.social_linkedin|| "Not Available";
document.getElementById("portfolioLink").textContent =selectedRegion.region_portfolio|| "Not Available";

        // Calculate and display age from date of birth, if available
        if (selectedRegion.member_date_of_birth) {
            const birthDate = new Date(selectedRegion.member_date_of_birth);
            const age = new Date().getFullYear() - birthDate.getFullYear();
            document.getElementById("basicAge").textContent = age||"Not Available";
        } else {
            document.getElementById("basicAge").textContent = "Not Provided";
        }
    } else {
        console.error("Member not found with the provided region_id.");
    }
}

// Load member data when the page loads
window.addEventListener('DOMContentLoaded',async () => {
    showLoader(); // Show loader immediately on page load

const regionId = getRegionIdFromUrl();
if (regionId) {
    try {
        await fetchRegionData(regionId); // Wait for data to be fetched
    } catch (error) {
        console.error("Failed to fetch member data:", error);
    } finally {
        hideLoader(); // Hide loader after data is fetched, whether successful or not
    }
} else {
    console.error("Member ID not found in URL.");
    hideLoader(); // Hide loader if member ID is missing
}
});


