              <div id="transactions-history">
                <table class="table text-nowrap">
                  <tbody>
                    <!-- Dynamic transaction entries will be inserted here -->
                  </tbody>
                </table>
              </div>
              <p id="more-entries" style="display: none">
                To see more entries, click on "View All".
              </p>
            </div>
            <script>
              document.addEventListener("DOMContentLoaded", async function () {
                const ordersResponse = await fetch(
                  "https://bni-data-backend.onrender.com/api/allOrders"
                );
                const ordersData = await ordersResponse.json();

                const linksResponse = await fetch(
                  "https://bni-data-backend.onrender.com/api/universaLlinks"
                );
                const linksData = await linksResponse.json();

                const transactionsHistoryDiv = document.querySelector(
                  "#transactions-history tbody"
                ); // Target the tbody for dynamic entries
                const moreEntriesText = document.getElementById("more-entries");

                // Clear existing content
                transactionsHistoryDiv.innerHTML = "";

                // Get only the most recent five orders
                const recentOrders = ordersData.slice(-5); // Get the last 5 entries

                // Loop through the recent orders
                recentOrders.forEach((order) => {
                  const link = linksData.find(
                    (link) => link.id === order.universal_link_id
                  );
                  if (link) {
                    const transactionRow = document.createElement("tr");
                    transactionRow.innerHTML = `
                                    <td>
                                        <div class="d-flex align-items-center gap-2">
                                            <div class="lh-1">
                                                <span class="fw-medium top-category-name">${link.universal_link_name}</span>
                                            </div>
                                        </div>
                                    </td>
                                    <td>
                                        <span class="fw-medium">${order.order_amount} ${order.order_currency}</span>
                                    </td>
                                `;
                    transactionsHistoryDiv.appendChild(transactionRow);
                  }
                });

                // Hide the message for more entries since we're fetching all entries now
                moreEntriesText.style.display = "none";
              });
            </script>
          </div>

