                      filteredTrainings.forEach((training) => {
                        const trainingDate = new Date(training.training_date);
                        const formattedDate = trainingDate.toLocaleDateString(); // Format the date as needed

                        // Construct the registration link dynamically (you may need to adjust this based on your actual data)
                        const registrationLink = `https://bninewdelhi.com/training-payments/${training.training_id}/bdbe4592-738e-42b1-ad02-beea957a3f9d/1`;

