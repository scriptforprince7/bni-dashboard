                    <script>
                      document.addEventListener(
                        "DOMContentLoaded",
                        function () {
                          const loggedInEmail =
                            localStorage.getItem("loggedInEmail"); // Get the logged-in email from local storage
                          const apiUrl =
                            "https://bni-data-backend.onrender.com/api/members";

                          fetch(apiUrl)
                            .then((response) => response.json())
                            .then((data) => {
                              // Find the member that matches the logged-in email
                              const member = data.find(
                                (member) =>
                                  member.member_email_address === loggedInEmail
                              );

                              if (member) {
                                // Update the accolades count in the HTML
                                const accoladesCount =
                                  member.accolades_id.length; // Get the length of accolades_id array
                                document.querySelector(
                                  ".accolades-count"
                                ).innerHTML = `<b>${accoladesCount}</b>`; // Update the HTML
                              } else {
                                console.error("Member not found");
                              }
                            })
                            .catch((error) =>
                              console.error(
                                "Error fetching member data:",
                                error
                              )
                            );
                        }
                      );
                    </script>
