        // Delay the logout action
        setTimeout(() => {
            localStorage.removeItem("token");
            window.location.href = "/";
        }, 3000);
      });

      // Handle session expiration logout
      function handleLogout() {
        // Configure toastr options
        toastr.options = {
            timeOut: 3000,
            closeButton: true,
            progressBar: true,
            positionClass: 'toast-top-right',
            preventDuplicates: true,
            onclick: null
        };
        
        // Show toaster and handle redirect
        toastr.success('Session expired! Redirecting to login...', 'Logged Out');
        
        // Delay the logout action
        setTimeout(() => {
            localStorage.removeItem("token");
            window.location.href = "/";
        }, 3000);
      }

      // Auth check
      if (!userEmail || !userLoginType) {
        window.location.href = "/";
