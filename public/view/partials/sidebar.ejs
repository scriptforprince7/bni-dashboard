  // API URL
  const apiUrl = "https://backend.bninewdelhi.com/api/getDisplayLogo";

  // Fetch data from the API
  fetch(apiUrl)
    .then(response => response.json())
    .then(data => {
      // Check if data is valid and contains the required field
      if (data.length > 0 && data[0].display_image_name) {
        const displayImageName = data[0].display_image_name;

        // Construct the image path
        const imagePath = `https://backend.bninewdelhi.com/uploads/dynamicLogo/${displayImageName}`;

        // Update the <img> src attribute
        const logoImg = document.getElementById("dynamic-logo");
        logoImg.src = imagePath;
      } else {
        console.error("No valid display image found in the API response.");
      }
    })
    .catch(error => {
      console.error("Error fetching display logo:", error);
    });
