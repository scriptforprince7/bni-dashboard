      console.log('Base64Url part:', base64Url);

      const base64 = base64Url.replace(/-/g, '+').replace(/_/g, '/');
      console.log('Processed base64:', base64);

      const payload = JSON.parse(window.atob(base64));
      console.log('Decoded payload:', payload);

      const email = payload.email;
      console.log('Extracted email:', email);

      if (email) {
        const emailName = email.split('@')[0];
        console.log('Name part of email:', emailName);

        // Try to find and update the display element
        const displayElement = document.querySelector('.slide:last-child .side-menu__label');
        console.log('Found display element:', !!displayElement);
        
        if (displayElement) {
          console.log('Current display text:', displayElement.textContent);
          displayElement.textContent = emailName;
          console.log('Updated display text to:', emailName);
        } else {
          console.error('Could not find the display element');
        }
      }
    } else {
      console.error('No token found in localStorage');
    }
  } catch (error) {
    console.error('Error in email extraction process:', error);
  }

  const currentPath = window.location.pathname;

